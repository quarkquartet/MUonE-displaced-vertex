Cell[BoxData[
 RowBox[{
  RowBox[{"\[Theta]Almaxcurve", "[", 
   RowBox[{"m_", ",", "El_"}], "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"cos\[Theta]c", "=", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"El", "/", "a"}], ")"}], "-", "Ec"}], 
      RowBox[{
       SqrtBox[
        RowBox[{"1", "-", 
         FractionBox["1", 
          SuperscriptBox["a", "2"]]}]], 
       SqrtBox[
        RowBox[{
         SuperscriptBox["Ec", "2"], "-", 
         SuperscriptBox["m", "2"]}]]}]]}], ";", "\[IndentingNewLine]", 
    RowBox[{"sin\[Theta]l", "=", 
     FractionBox[
      RowBox[{
       SqrtBox[
        RowBox[{
         SuperscriptBox["Ec", "2"], "-", 
         SuperscriptBox["m", "2"]}]], 
       SqrtBox[
        RowBox[{"1", "-", 
         SuperscriptBox["cos\[Theta]c", "2"]}]]}], 
      SqrtBox[
       RowBox[{
        SuperscriptBox["El", "2"], "-", 
        SuperscriptBox["m", "2"]}]]]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Maximize", "[", 
      RowBox[{
       RowBox[{"{", 
