   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"T", "[", "x_", "]"}], ":=", 
      FractionBox[
       RowBox[{"2", "m"}], 
       RowBox[{"(", 
        RowBox[{
         FractionBox[
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"E1", "+", "m"}], ")"}], "2"], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["E1", "2"], "-", 
