          SqrtBox[
           RowBox[{
            SuperscriptBox["E1", "2"], "-", 
            SuperscriptBox["M", "2"]}]], "-", 
          RowBox[{
           SqrtBox[
            RowBox[{
             RowBox[{"T", "[", "x", "]"}], " ", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"2", "m"}], " ", "+", 
               RowBox[{"T", "[", "x", "]"}]}], ")"}]}]], 
